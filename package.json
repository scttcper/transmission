{
  "name": "@ctrl/transmission",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for transmission using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/transmission",
  "keywords": [
    "transmission",
    "typescript"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.0.1",
    "got": "^9.6.0"
  },
  "devDependencies": {
    "@types/got": "9.4.1",
    "@types/jest": "24.0.9",
    "@types/node": "11.10.4",
    "@types/p-wait-for": "2.0.0",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "eslint": "5.15.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.8.0",
    "eslint-plugin-import": "2.16.0",
    "jest": "24.1.0",
    "p-wait-for": "2.0.1",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "24.0.0",
    "typedoc": "0.14.2",
    "typescript": "3.3.3333"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
