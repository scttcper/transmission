{
  "name": "@ctrl/transmission",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for transmission using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/transmission",
  "keywords": [
    "transmission",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/magnet-link": "^3.1.1",
    "@ctrl/shared-torrent": "^4.3.1",
    "@ctrl/url-join": "^2.0.2",
    "got": "^12.5.0"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.5.0",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "18.7.18",
    "@vitest/coverage-c8": "0.23.4",
    "c8": "7.12.0",
    "p-wait-for": "5.0.0",
    "typedoc": "0.23.15",
    "typescript": "4.8.3",
    "vitest": "0.23.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageProvider": "v8"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=14.16"
  }
}
